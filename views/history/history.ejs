<div class="container-fluid">
    <div class="d-sm-flex align-items-center justify-content-between mb-3">
      <h1 class="page-title">TRANSACTION HISTORY</h1>
    </div>
  
    <div class="card shadow mb-4">
      <div class="card-header py-3">
        <div class="row mb-4">
          <div class="col-md-3">
            <h6 class="m-0 font-weight-bold table-title">User Table</h6>
          </div>
          <div class="col-md-6">
            <input
              id="search"
              name="search"
              type="text"
              class="form-control"
              placeholder="Search"
              aria-label="search"
              aria-describedby="basic-addon1"
            />
          </div>
        </div>
        <div class="card-body">
          <div class="table-responsive-sm">
            <table id="history-table" class="table table-bordered table-sm table-hover">
              <thead class="table-dark">
                <th>Transaction id</th>
                <th>Date</th>
                <th>Employee ID</th>
                <th>Amount</th>
                <th>Created By</th>
                <th>Created Date</th>
                <th>Actions</th>
              </thead>
              <tbody class="table-light"></tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
  